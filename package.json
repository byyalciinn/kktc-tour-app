{
  "name": "tour-app",
  "version": "1.0.0",
  "description": "KKTC Tour App - Discover tours in North Cyprus",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json}\"",
    "check:i18n": "node scripts/check-i18n.js",
    "validate": "npm run typecheck && npm run lint:check && npm run test"
  },
  "keywords": [
    "expo",
    "react-native",
    "tour",
    "kktc",
    "travel"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@expo/ui": "~0.2.0-beta.7",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@supabase/supabase-js": "^2.86.0",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.8",
    "base64-arraybuffer": "^1.0.2",
    "expo": "^54.0.25",
    "expo-blur": "^15.0.7",
    "expo-camera": "~17.0.9",
    "expo-constants": "~18.0.10",
    "expo-device": "~8.0.9",
    "expo-glass-effect": "~0.1.7",
    "expo-haptics": "~15.0.7",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-local-authentication": "~16.0.4",
    "expo-localization": "^17.0.7",
    "expo-location": "~19.0.7",
    "expo-mesh-gradient": "~0.4.7",
    "expo-network": "~8.0.4",
    "expo-notifications": "~0.32.13",
    "expo-router": "~6.0.15",
    "expo-screen-capture": "~8.0.4",
    "expo-secure-store": "^15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "i18next": "^25.6.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.3.5",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0",
    "typescript": "~5.9.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0",
    "prettier": "^3.2.0",
    "react-test-renderer": "19.1.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|zustand)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  }
}
